{
  "name": "pathofchallenges",
  "version": "1.0.0",
  "description": "Program for Path of Exile League Challenges",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . PathofChallenges --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"PathofChallenges\"",
    "package-linux": "electron-packager . PathofChallenges --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Leighton Jonker",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.2.1",
    "electron": "^4.0.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6"
  },
  "devDependencies": {
    "electron-packager": "^13.0.1"
  }
}
